{
  "name": "boss-balloon",
  "version": "1.0.0",
  "description": "BossBalloon.io",
  "main": "server/index.js",
  "scripts": {
    "asbuild:client_debug": "asc client/client.ts --target client_debug --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:client_release": "asc client/client.ts --target client_release --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:server_debug": "asc server/server.ts --target server_debug --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:server_release": "asc server/server.ts --target server_release --runtime minimal --exportTable --exportRuntime --importMemory",
    "release": "npm run asbuild:client_release && npm run asbuild:server_release",
    "debug": "npm run asbuild:client_debug && npm run asbuild:server_debug",
    "server": "node server/index.js",
    "genkey": "openssl req -x509 -newkey rsa:2048 -keyout key.pem -out cert.pem -days 100 -nodes",
    "asbuild:watch": "nodemon --ext ts -w client -x npm run asbuild:client_debug",
    "server:watch": "nodemon --ext ts -w client -x node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catid/boss-balloon.git"
  },
  "keywords": [
    "game"
  ],
  "author": "catid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catid/boss-balloon/issues"
  },
  "homepage": "https://github.com/catid/boss-balloon#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.18.27"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.27",
    "assemblyscript-json": "^1.0.0",
    "express": "^4.17.1",
    "node-datachannel": "murat-dogan/node-datachannel",
    "nodemon": "^2.0.7",
    "rezidb": "^1.0.1",
    "ws": "^7.4.5"
  }
}
