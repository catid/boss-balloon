{
  "name": "boss-balloon",
  "version": "1.0.0",
  "description": "BossBalloon.io",
  "main": "server/index.js",
  "scripts": {
    "asbuild:client_debug": "asc client/main.ts --target client_debug --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:client_release": "asc client/main.ts --target client_release --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:server_debug": "asc server/main.ts --target server_debug --runtime minimal --exportTable --exportRuntime --importMemory",
    "asbuild:server_release": "asc server/main.ts --target server_release --runtime minimal --exportTable --exportRuntime --importMemory",
    "release": "npm run asbuild:client_release && npm run asbuild:server_release",
    "debug": "npm run asbuild:client_debug && npm run asbuild:server_debug",
    "server": "pm2 start server/index.js --watch",
    "genkey": "openssl req -x509 -newkey rsa:2048 -keyout key.pem -out cert.pem -days 100 -nodes",
    "asbuild:watch": "nodemon --ext ts -w client -x npm run asbuild:client_debug",
    "server:watch": "nodemon --ext ts -w client -x node server/index.js",
    "client": "npx google-closure-compiler --js client/js/index.js client/js/loader.esm.js client/js/ASWebGLue2.js --js_output_file=client/deploy/bossballoon.js --strict_mode_input=false --compilation_level ADVANCED",
    "deploy": "./scripts/deploy.sh",
    "ubuntu": "sudo apt install build-essential cmake"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catid/boss-balloon.git"
  },
  "keywords": [
    "game"
  ],
  "author": "catid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catid/boss-balloon/issues"
  },
  "homepage": "https://github.com/catid/boss-balloon#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.18.27",
    "express": "^4.17.1",
    "node-datachannel": "^0.1.3"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.27",
    "assemblyscript-json": "^1.0.0",
    "google-closure-compiler": "^20210505.0.0",
    "nodemon": "^2.0.7",
    "rezidb": "^1.0.1",
    "ws": "^7.4.5"
  }
}
